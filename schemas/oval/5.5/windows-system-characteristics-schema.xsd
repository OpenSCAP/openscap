                                        <xsd:documentation>Grants the right to read extended attributes.</xsd:documentation>
                                        <xsd:appinfo>
                                             <sch:pattern id="feritemfile_read_ea">
                                                  <sch:rule context="win-sc:fileeffectiverights_item/win-sc:file_read_ea">
